<p id="notice"><%= notice %></p>

<h1>Extintors</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @extintors.each do |extintor| %>
      <tr>
        <td><%= link_to 'Show', extintor %></td>
        <td><%= link_to 'Edit', edit_extintor_path(extintor) %></td>
        <td><%= link_to 'Destroy', extintor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Extintor', new_extintor_path %>
